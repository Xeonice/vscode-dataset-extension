{
	"name": "c7n-dataset-plugin",
	"displayName": "c7n-dataset-plugin",
	"description": "choerodon-ui/pro 语法补全插件",
	"repository": "https://github.com/Xeonice/vscode-dataset-extension",
	"keywords": [
		"dataset",
		"c7n-ui",
		"choerodon-ui"
	],
	"version": "0.1.16",
	"publisher": "handMS",
	"icon": "images/choerodon-community.png",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:typescript",
		"onLanguage:javascriptreact",
		"onLanguage:typescriptreact",
		"onCommand:c7n.search"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "c7n.search",
				"title": "c7n.search"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "c7n.search",
					"group": "navigation"
				}
			]
		},
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/script.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/script.json"
			},
			{
				"language": "typescript",
				"path": "./snippets/script.json"
			},
			{
				"language": "typescriptreact",
				"path": "./snippets/script.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/reactComponent.json"
			},
			{
				"language": "typescriptreact",
				"path": "./snippets/reactComponent.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"docs": "cd build && node generate.js && cd .."
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"eslint": "^6.8.0",
		"fs-extra": "^9.0.1",
		"glob": "^7.1.6",
		"js-beautify": "^1.11.0",
		"mocha": "^7.1.2",
		"remark-parse": "^8.0.2",
		"remark-stringify": "^8.1.0",
		"typescript": "^3.8.3",
		"unified": "^9.0.0",
		"vscode": "^1.1.37",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"lodash": "^4.17.19"
	}
}
